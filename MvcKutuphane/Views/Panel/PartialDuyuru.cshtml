@using MvcKutuphane.Models.Entity
@model List<TBLDUYURULAR>

@foreach (var item in Model)
{


    <div class="post">

        <div class="user-block">

            <img class="img-circle img-bordered-sm" src="~/AdminLTE-1/dist/img/icons.png" alt="user image">

            <span class="username">

                <a href="#">@item.KATEGORI</a>

                <a href="#" class="float-right btn-tool"><i class="fas fa-times"></i></a>

            </span>

            <span class="description">@(Convert.ToDateTime(item.TARIH).ToString("dd/MM/yyyy"))</span>

        </div>

        <!-- /.user-block -->

        <p>

         @item.ICERIK

        </p>     

       

    </div>
}